{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "description": "CSV Processing API - Upload, parse, validate, and store financial CSV files",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "vercel-build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:connection": "tsx src/config/test-connection.ts"
  },
  "keywords": [
    "csv",
    "api",
    "fastify",
    "typescript",
    "fintech"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/papaparse": "^5.5.2",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.5.17",
    "@vitest/ui": "^4.0.17",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "papaparse": "^5.5.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
